<p id="notice"><%= notice %></p>
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Listing Assets</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="dataTable_wrapper">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                      <thead>
                        <tr>
                          <th>Type</th>
                          <th>Name</th>
                          <th colspan="3"></th>
                        </tr>
                      </thead>

                      <tbody>
                        <% @azzets.each do |azzet| %>
                          <tr>
                            <td><%= azzet.Type %></td>
                            <td><%= azzet.Name %></td>
                            <td><%= link_to 'Show', azzet %></td>
                            <td><%= link_to 'Edit', edit_azzet_path(azzet) %></td>
                            <td><%= link_to 'Destroy', azzet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                          </tr>
                        <% end %>
                      </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>

<br>

<%= link_to 'New Azzet', new_azzet_path %>
